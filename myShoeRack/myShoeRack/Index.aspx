        <% if (Request.Cookies["Location"] != null && Session["LoggedIn"] != null && Request.Cookies["AuthToken"] != null && Session["AuthToken"] != null)
            { %>
            <div class="modal fade" id="LoginSecurity" tabindex="-1" role="dialog" aria-labelledby="LoginSecurityLabel" aria-hidden="true">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="LoginSecurityLabel">Recent Login Information - Is This You?</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="modalClose">
                      <span aria-hidden="true">&times;</span>
                    </button>
                  </div>
                  <div class="modal-body">
                    <h6>Someone has logged into your account recently : </h6>
                      <br />
                      <p>Device Name &#58; <% Response.Write(Request.Cookies["Location"]["DeviceName"]); %></p>
                      <p>Country/City &#58; <% Response.Write("<img src='" + Request.Cookies["Location"]["Flag"] + "'> " + Request.Cookies["Location"]["Country"] + ", " + Request.Cookies["Location"]["City"]); %></p>
                      <p>Computer&#8217;s Public IP &#58; <% Response.Write(Request.Cookies["Location"]["Ip"]); %></p>
                      <p>Date &amp; Time &#58; <% Response.Write(Request.Cookies["Location"]["CurrentTime"]); %> GMT</p>
                  </div>
                  <div class="modal-footer">
                  </div>
                </div>
              </div>
            </div>

            <script>
                $(window).on('load',function(){
                    $('#LoginSecurity').modal('show');
                });
            </script>
        <% } %>
